<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="604.0" prefWidth="464.0" xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.FahrzeugErstellenController">

    <children>
        <Label layoutX="70.0" layoutY="44.0" text="Neues Fahrzeug erstellen">
            <font>
                <Font size="18.0" />
            </font>
        </Label>
        <TextField fx:id="herstellerField" layoutX="86.0" layoutY="100.0" promptText="Hersteller" />
        <TextField fx:id="modellField" layoutX="86.0" layoutY="150.0" promptText="Modell" />
        <TextField fx:id="ausstattungField" layoutX="86.0" layoutY="200.0" promptText="Ausstattungsvariante" />
        <TextField fx:id="leistungField" layoutX="86.0" layoutY="250.0" promptText="Leistung in KW" />
        <TextField fx:id="kraftstoffField" layoutX="86.0" layoutY="300.0" promptText="Kraftstoffart" />
        <TextField fx:id="baujahrField" layoutX="86.0" layoutY="350.0" promptText="Baujahr" />
        <TextField fx:id="getriebeField" layoutX="86.0" layoutY="400.0" promptText="Art des Getriebes" />
        <TextField fx:id="sitzplaetzeField" layoutX="86.0" layoutY="450.0" promptText="Sitzplätze" />
        <Button fx:id="speichernButton" layoutX="371.0" layoutY="551.0" onAction="#speichernButtonClicked" text="Speichern" />
      <Button fx:id="zurueckButton" layoutX="86.0" layoutY="551.0" mnemonicParsing="false" onAction="#zurueckButtonClicked" text="Zurück" />
      <ComboBox fx:id="sharingComboBox" layoutX="86.0" layoutY="500.0" prefHeight="26.0" prefWidth="160.0" />
    </children>
</AnchorPane>
